{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Bucket",
  "version": "0.1.0",
  "identifier": "com.bucket-app.dev",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1422",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },

  "app": {
    "macOSPrivateApi": true,
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Bucket",
        "width": 750,
        "height": 525,
        "resizable": true
      }
    ],
    "security": {
      "csp": null,
      "capabilities": []
    }
  },
  "plugins": {
    "dialog": null,
    "fs": null,
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDlEMTg1Njc4NjNDQjk2RUQKUldUdGxzdGplRllZbmZMN1ZWM3FXK2ROZ1luZEt2VktaVWRlajAyZGtpc2Y4TnRNS2s5U0VQL24K",
      "endpoints": [
        "https://github.com/twentynineteen/ingest-tauri/releases/latest/download/latest.json"
      ]
    }
  },
  "bundle": {
    "createUpdaterArtifacts": true,
    "resources": ["assets"],
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "entitlements": "tauri.entitlements.mac.plist",
      "exceptionDomain": "localhost",
      "signingIdentity": null
    }
  }
}
